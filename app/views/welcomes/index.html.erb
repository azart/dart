<h1>Listing welcomes</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Phone</th>
    <th>Description</th>
    <th>Locale</th>
    <th>Seo title</th>
    <th>Seo keywords</th>
    <th>Seo description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @welcomes.each do |welcome| %>
  <tr>
    <td><%= welcome.title %></td>
    <td><%= welcome.phone %></td>
    <td><%= welcome.description %></td>
    <td><%= welcome.locale %></td>
    <td><%= welcome.seo_title %></td>
    <td><%= welcome.seo_keywords %></td>
    <td><%= welcome.seo_description %></td>
    <td><%= link_to 'Show', welcome %></td>
    <td><%= link_to 'Edit', edit_welcome_path(welcome) %></td>
    <td><%= link_to 'Destroy', welcome, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Welcome', new_welcome_path %>
